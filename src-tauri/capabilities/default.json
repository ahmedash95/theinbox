{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "process:allow-restart",
    "core:menu:default",
    "core:window:allow-start-dragging",
    "core:window:allow-set-focus",
    "opener:default",
    "opener:allow-open-path",
    "updater:default"
  ]
}
